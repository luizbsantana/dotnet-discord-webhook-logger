<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<NoWarn>$(NoWarn);1591</NoWarn>
		<PackageId>Discord.Webhook.Logger</PackageId>
		<Version>1.0.3</Version>
		<Authors>Luiz Santana</Authors>
		<Description>Use discord webhook to send application logs</Description>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="CSharpDiscordWebhook.NET" Version="2.7.1" />
		<PackageReference Include="Brazil.DateTime" Version="1.0.1" />
	</ItemGroup>
</Project>